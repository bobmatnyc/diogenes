{
  "name": "diogenes",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "version:major": "pnpm version major && git push && git push --tags",
    "version:minor": "pnpm version minor && git push && git push --tags",
    "version:patch": "pnpm version patch && git push && git push --tags",
    "version:prerelease": "pnpm version prerelease --preid=rc && git push && git push --tags",
    "version:current": "echo Current version: v$(node -p \"require('./package.json').version\")",
    "changelog": "echo 'Generating changelog...' && git log --oneline --decorate --color",
    "release": "pnpm run build && pnpm run version:patch",
    "release:minor": "pnpm run build && pnpm run version:minor",
    "release:major": "pnpm run build && pnpm run version:major",
    "typecheck": "tsc --noEmit",
    "lint:biome": "biome check .",
    "format:biome": "biome check --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
